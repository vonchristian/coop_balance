    "jquery-timepicker": "^1.3.3",
    "mini-css-extract-plugin": "^2.7.6",
    "nodemon": "^3.0.1",
    "postcss": "^8.4.27",
    "postcss-cli": "^10.1.0",
    "preline": "^2.7.0",
    "sass": "^1.64.1",
    "sass-loader": "^13.3.2",
