<div class="modal-header">
  <h4 class="modal-title card-title" id="mainModalLabel">
    Cooperative Services
  </h4>
  <button type="button" class="close float-right" data-dismiss="modal"><span aria-hidden="true">&times;</span><span class="sr-only">Close</span></button>
</div>
<%= simple_form_for [current_cooperative, @cooperative_service], html: { autocomplete: "off", data: { modal: true } }, remote: request.xhr?, url: :cooperative_services do |f| %>
	<div class="modal-body">
		<%= f.input :title %>
	</div>
	<div class="modal-footer">
	  <%= f.submit "Create Cooperative Service", class: 'btn btn-danger float-right' %>
	</div>
<% end %>
