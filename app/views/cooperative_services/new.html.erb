<div class="row">
	<div class="col-md-6 offset-md-3">
		<div class="card">
			<div class="card-header">
				<h3 class="card-title">Cooperative Services</h3>
			</div>
			<div class="card-body">
				<%= simple_form_for [current_cooperative, @cooperative_service], url: :cooperative_services do |f| %>
				  <%= f.input :title %>
				  <hr>
				  <%= link_to "Cancel", management_module_settings_configurations_path, class: 'btn btn-default' %>
				  <%= f.submit "Create Cooperative Service", class: 'btn btn-primary float-right' %>
				<% end %>
			</div>
		</div>
	</div>
</div>
