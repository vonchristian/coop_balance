<% @members.each do |member| %>
  <% cache member do %>
    <div class="row">
      <div class="col-md-2">
        <center>            <%= member_avatar(member, style: 'width:60px;height:60px;', class: 'media-object img-circle') %>


      </div>
      <div class="col-md-6">
        <%= link_to member.full_name, management_department_member_path(member) %>
      </div>
    </div>
    <hr>
  <% end %>
<% end %>
