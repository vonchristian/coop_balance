<div class="row">
  <div class="col-md-6">
    <div class="card">
      <div class="card-header">
        <div class="row">
          <div class="col-md-10">
            <h3 class="card-title"> Grace Period Configuration </h3>
          </div>
          <div class="col-md-2">
            <%= link_to new_management_module_grace_period_path do %>
              <span class="btn btn-sm btn-outline-danger float-right" data-toggle="tooltip" title="Set Grace Period"><i class="fa fa-edit"> Set</i></span>
            <% end %>
          </div>
        </div>
      </div>
      <div class="card-body">
        
      </div>
    </div>
  </div>
  <div class="col-md-6">
    <div class="card">
      <div class="card-header">
        <div class="row">
          <div class="col-md-10">
            <h3 class="card-title"> Savings Account Configuration </h3>
          </div>
          <div class="col-md-2">
            <%= link_to new_management_module_settings_savings_account_config_path do %>
              <span class="btn btn-sm btn-outline-danger float-right" data-toggle="tooltip" title="Set Savings Accounts Configuration"><i class="fa fa-edit"> Set</i></span>
            <% end %>
          </div>
        </div>
      </div>
      <div class="card-body">
        
      </div>
    </div>
  </div>
</div>
<div class="row">
  <div class="col-md-6">
    <div class="card">
      <div class="card-header">
        <div class="row">
          <div class="col-md-9">
            <h3 class="card-title"> Programs / Fee Schedules </h3>
          </div>
          <div class="col-md-3">
            <%= link_to new_management_module_program_path do %>
              <span class="btn btn-outline-danger btn-sm float-right" title="New Program/Fee" data-toggle="tooltip">
                <i class="fa fa-plus-circle"></i> New
              </span>
            <% end %>
          </div>
        </div>
      </div>
      <div class="card-body">
      </div>
    </div>
  </div>
  <div class="col-md-6">
    <% @programs.each do |program| %>
      <%= program.description %>
      <%= program.contribution %>
      <%= program.account.name %>
    <% end %>
  </div>
</div>
<div class="row">
  <div class="col-md-6">
    <div class="card">
      <div class="card-header">
        <div class="row">
          <div class="col-md-9">
            <h3 class="card-title"> Cooperative Services </h3>
          </div>
          <div class="col-md-3">
            <%= link_to new_cooperative_service_path do %>
              <span class="btn btn-sm btn-outline-danger float-right" data-toggle="tooltip" title="New Cooperative Service"><i class="fa fa-plus-circle"> New</i></span>
            <% end %>
          </div>
        </div>
      </div>
      <div class="card-body">

      </div>
    </div>
  </div>
  <div class="col-md-6">
    <div class="card">
      <div class="card-header">
        <div class="row">
          <div class="col-md-6">
            <h3 class="card-title"> Store Fronts</h3>
          </div>
          <div class="col-md-6">
            <%= link_to new_management_module_settings_store_front_path do  %>
              <span class="btn btn-danger btn-sm float-right" data-toggle="tooltip" title="New Savings Product"><i class="fa fa-plus"></i> New </span>
            <% end %>
          </div>
        </div>
      </div>
      <div class="card-body">
        <% StoreFront.all.each do |store_front| %>
          <%= store_front.name %>
        <% end %>
      </div>
    </div>
  </div>
</div>