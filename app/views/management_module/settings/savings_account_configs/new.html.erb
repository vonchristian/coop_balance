<%=simple_form_for @savings_account_config, url: :management_module_settings_savings_account_configs do |f| %>
  <%= f.input :closing_account_id, collection: AccountingModule::Account.all.map{|a| [a.name, a.id] } %>
  <%= f.input :interest_account_id, collection: AccountingModule::Account.all.map{|a| [a.name, a.id] } %>
  <%= f.input :closing_account_fee, as: :string %>
  <%= f.submit "Save Configuration" %>
<% end %>
