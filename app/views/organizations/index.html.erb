<div class="row">
  <div class="col-md-12">
    <div class="card">
      <div class="card-header">
        <div class="row">
          <div class="col-md-6">
            <h3 class="card-title"> Organizations </h3>
          </div>
          <div class="col-md-5">
            <%= link_to 'New Organization', new_organization_path, class: "btn btn-primary float-right" %>
          </div>
        </div>
      </div>
    </div>
      <% @organizations.each do |organization| %>
        <%= render 'organizations/partials/header', organization: organization %>
      <% end %>
    </div>
  </div>
</div>
