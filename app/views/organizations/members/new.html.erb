<% @members.each do |member| %>
  <%= member.name %>

  <%= simple_form_for [@organization, @member], url: :organization_members do |f| %>
    <%= f.input :organization_membership_id, input_html: { value: member.id } %>
    <%= f.input :organization_membership_type, input_html: { value: member.class } %>
    <%= f.submit "Add Member" %>
  <% end %>
<% end %>
<%=will_paginate @members %>
