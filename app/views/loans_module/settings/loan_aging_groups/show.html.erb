<%= link_to 'Edit', edit_loans_module_settings_loan_aging_group_path(@loan_aging_group) %>
<%= link_to 'Set Office Loan Product', new_loans_module_settings_loan_aging_group_office_loan_product_path(@loan_aging_group) %>
<%= link_to 'Loans', loans_module_loan_aging_group_loans_path(@loan_aging_group) %>
<h3 class='title-up'> Loan Products </h3>
<div class='table table-responsive'>
  <table class='table table-hover'>
    <thead>
      <tr>
        <th> Loan Product </th>
        <th> Account </th>
      </tr>
    </thead>
    <tbody>
      <% @loan_aging_group.office_loan_products.each do |office_loan_product| %>
        <tr>
          <td><%= office_loan_product.name %></td>
        </tr>
      <% end %>
    </tbody>
  </table>
</div>
