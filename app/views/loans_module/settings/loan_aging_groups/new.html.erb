<%= simple_form_for @loan_aging_group, url: :loans_module_settings_loan_aging_groups do |f| %>
  <%= f.input :title %>
  <%= f.input :start_num %>
  <%= f.input :end_num %>
  <%= f.input :receivable_ledger_id, collection: current_office.ledgers.asset.map{ |a| [a.name, a.id] }, input_html: { class: 'chosen-select' } %>
  
  <%= f.submit 'Create Loan Aging Group' %>
<% end %>
