<div class="card">
  <div class="card-body">
    <% if policy([:members, :savings_account]).new? %>
     <%=link_to 'New Savings Account', new_savings_account_application_path(depositor_id: @member.id, depositor_type: @member.class.to_s), class: "btn btn-primary float-right" %>
    <% end %>
  </div>
</div>

    <% @member.savings.each do |savings_account| %>
      <%= render 'savings_accounts/partials/header', savings_account: savings_account %>
    <% end %>
