<div class="row">
  <div class="col-md-8 col-md-offset-2">
    <div class="box box-solid">
      <div class="box-header">
        <h3 class="box-title"> Real Property Details </h3>
      </div>
      <div class="box-body">

        <div class="row">
          <div class="col-md-10 col-md-offset-1">
            <p class="text-muted"><b> Appraisal </b></p>
            <p> Market Value</p>
            <hr>
            <% @real_property.pictures.each do |image|%>
              <%=image_tag(image.image.url(:large), class: "img-square") %>
            <% end %>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>
