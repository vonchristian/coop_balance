Generate Report 
<%= form_tag({:action => "index", format: 'pdf', controller: "employees/entries", employee_id: @employee.id, from_date: params[:from_date], to_date: params[:to_date]}, {:method => :get})  do %>
              <div class="row">
                <div class="col-md-3">
                  <div class="form-group">
                    <%= text_field_tag :from_date, @from_date, :class => 'datepicker form-control border-input', :placeholder => "From " %>
                  </div>
                </div>
                <div class="col-md-3">
                  <div class="form-group">
                    <%= text_field_tag :to_date, @to_date, :class => 'form-control border-input datepicker', :placeholder => "To " %>
                  </div>
                </div>
                <div class="col-md-1">
                  <button type="submit" class="btn btn-default"> Go</button>
                </div>
              </div>
            <% end %>
<%=link_to 'Transactions Report(PDF)', employee_entries_path(@employee, format: 'pdf', from_date: params[:from_date], to_date: params[:to_date])  %>

<% @entries.each do |entry| %>
  <%= entry.entry_date %>
<% end %>