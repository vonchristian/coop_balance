
<%= simple_form_for [@time_deposit, @renewal], url: :time_deposit_term_extensions do |f| %>
  <%= f.error_notification %>
  <%= f.input :time_deposit_id, input_html: { value: @time_deposit.id } %>
  <%= f.input :member_id, input_html: { value: @time_deposit.depositor.id } %>
  <%= f.input :date_deposited, label: "Date Renewed:", as: :string, input_html: { class: "datepicker" } %>
  <%= f.input :amount, input_html: { value: @time_deposit.balance } %>
<%= f.input :number_of_days %>
  <%= f.submit %>
<% end %>
