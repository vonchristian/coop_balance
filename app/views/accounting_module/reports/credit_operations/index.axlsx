wb = xlsx_package.workbook
wb.add_worksheet(name: "Savings") do |sheet|
sheet.add_row ["Credit Operations", "As of #{@to_date.strftime("%B %e, %Y")}"]
sheet.add_row ["Member", "Name", "Balance"]
sheet.add_row @accounts.map{|a| "#{a.name}"}

@members.each do |member|
  sheet.add_row [
    member.full_name
  ]
end


end
