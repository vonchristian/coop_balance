
<div class="row">
  <div class="col-md-12">
    <div class="box box-default">
      <div class="box-header">
        <h3 class="box-title"> <%= params[:entry_type].try(:titleize) || "All" %> Entries </h3>
      </div>
      <div class="box-body">
        <div class="table table-responsive">
          <table class="table table-striped">
            <thead>
              <tr>
                <th> Date </th>
                <th> Description </th>
                <th> Reference Number </th>
                <th> Amount </th>
                <th> Member </th>
              </tr>
            </thead>
            <tbody>
              <% @account.entries.each do |entry| %>
                <% cache entry do %>
                  <%= render 'accounting_module/entries/entry', entry: entry %>
                <% end %>
              <% end %>
            </tbody>
          </table>
          