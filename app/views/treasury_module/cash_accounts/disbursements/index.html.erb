<div class="container">
  <div class="container-fluid">
    <div class="card">
      <div class="card-header">
        <h3 class="card-title"> Generate Report </h3>
      </div>
      <div class="card-body">
        <%= form_tag({format: 'xlsx', :action => 'index', controller: "treasury_module/cash_accounts/receipts"}, {:method => :get, :class => 'form-inline'}) do%>
          <div class="form-group">
            <%= text_field_tag :to_date, @from_date, :class => 'datepicker form-control' %>
            <%= text_field_tag :to_date, @to_date,   :class => 'datepicker form-control' %>
          </div>
          <button type="submit" class="btn btn-default">Get Report (Excel)</button>
        <% end %>
      </div>
    </div>
  </div>
</div>
<% @entries.each do |entry| %>
  <%= render 'accounting_module/entries/partials/header', entry: entry, cached: true %>
<% end %>
